datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id Int @id @default(autoincrement())
  firstname String
  lastName String?
  username String @unique
  email String @unique
  password String
  location String
  avatar String
  githubUsername String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}